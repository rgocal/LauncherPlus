<resources>
    <bool name="config_largeHeap">false</bool>
    <bool name="is_large_screen">false</bool>
    <bool name="allow_rotation">false</bool>
    <bool name="lock_workspace">false</bool>

<!-- DragController -->
    <integer name="config_flingToDeleteMinVelocity">-1500</integer>

<!-- AllApps/Customize/AppsCustomize -->
    <!-- Which default transition effect to use on the AppsCustomize -->
    <string name="config_drawerDefaultTransitionEffect">Stack</string>
    <!-- The alpha of the AppsCustomize bg in spring loaded mode -->
    <integer name="config_appsCustomizeSpringLoadedBgAlpha">45</integer>
    <integer name="config_workspaceUnshrinkTime">300</integer>
    <!-- Out of 100, the percent to shrink the workspace during spring loaded mode. -->
    <integer name="config_workspaceSpringLoadShrinkPercentage">80</integer>

    <!-- Fade/zoom in/out duration & scale in the AllApps transition.
         Note: This should be less than the workspaceShrinkTime as they happen together. -->
    <integer name="config_appsCustomizeZoomInTime">350</integer>
    <integer name="config_appsCustomizeZoomOutTime">600</integer>
    <integer name="config_appsCustomizeZoomScaleFactor">7</integer>
    <integer name="config_appsCustomizeFadeInTime">250</integer>
    <integer name="config_appsCustomizeFadeOutTime">200</integer>
    <integer name="config_appsCustomizeWorkspaceShrinkTime">300</integer>
    <integer name="config_appsCustomizeWorkspaceAnimationStagger">40</integer>
    <integer name="config_workspaceAppsCustomizeAnimationStagger">100</integer>
 
    <!-- Fade/zoom in duration PreviewLayout transition. -->
    <integer name="config_previewsZoomInTime">350</integer>
    <integer name="config_previewsZoomOutTime">600</integer>
    <integer name="config_previewsZoomScaleFactor">7</integer>
    <integer name="config_previewsFadeInTime">250</integer>
    <integer name="config_previewsFadeOutTime">200</integer>

    <!-- Tab transition animation duration -->
    <integer name="config_tabTransitionDuration">250</integer>

    <!-- The slope, in percent, of the drag movement needed to drag an item out of
         AppsCustomize (y / x * 100%)  -->
    <integer name="config_appsCustomizeDragSlopeThreshold">150</integer>

    <!-- Batch loading for loading in LauncherModel -->
    <integer name="config_allAppsBatchLoadDelay">0</integer>
    <integer name="config_allAppsBatchSize">0</integer>

<!-- Workspace -->
    <!-- Whether or not the drop targets drop down as opposed to fade in -->
    <bool name="config_useDropTargetDownTransition">true</bool>
    <!-- Which default transition effect to use on the workspace -->
    <string name="config_workspaceDefaultTransitionEffect">CubeOut</string>
    <string name="config_dockDefaultTransitionEffect">Standard</string>
    <!-- Whether or not to fade the side pages -->
    <bool name="config_workspaceDefualtFadeInAdjacentScreens">true</bool>
    <bool name="config_drawerFadeAdjacentScreens">true</bool>
    <!-- Whether to show outlines on the screens when scrolling -->
    <bool name="config_workspaceDefaultShowOutlines">false</bool>

    <!-- The transition duration for the background of the drop targets -->
    <integer name="config_dropTargetBgTransitionDuration">0</integer>

    <!-- The duration (in ms) of the fade animation on the object outlines, used when
         we are dragging objects around on the home screen. -->
    <integer name="config_dragOutlineFadeTime">900</integer>

    <!-- The alpha value at which to show the most recent drop visualization outline. -->
    <integer name="config_dragOutlineMaxAlpha">128</integer>

    <!-- Parameters controlling the animation for when an item is dropped on the home screen,
         and it animates from its old position to the new one. -->
    <integer name="config_dropAnimMinDuration">100</integer>
    <integer name="config_dropAnimMaxDuration">500</integer>

    <!-- The duration of the UserFolder opening and closing animation -->
    <integer name="config_folderAnimDuration">120</integer>

    <!-- The distance at which the animation should take the max duration -->
    <integer name="config_dropAnimMaxDist">800</integer>

    <!-- Properties controlling the workspace fade-out during dragging -->
    <integer name="config_dragFadeOutAlpha">80</integer>
    <integer name="config_dragFadeOutDuration">250</integer>

    <!-- Camera distance for the overscroll effect -->
    <integer name="config_cameraDistance">6500</integer>

    <!-- Folder max bounds and max number of items. Note: folder_max_count_x * folder_max_count_y
         >= folder_max_num_items. When these are set to -1, they are automatically determined. -->
    <integer name="folder_max_count_x">4</integer>
    <integer name="folder_max_count_y">4</integer>
    <integer name="folder_max_num_items">16</integer>

    <!-- Workspace grid size -->
    <integer name="config_defaultWorkspaceRows">4</integer>
    <integer name="config_maxWorkspaceRows">12</integer>
    <integer name="config_minWorkspaceRows">3</integer>
    <integer name="config_defaultWorkspaceColumns">4</integer>
    <integer name="config_maxWorkspaceColumns">12</integer>
    <integer name="config_minWorkspaceColumns">3</integer>
    <integer name="config_defaultWorkspaceRowsLand">4</integer>
    <integer name="config_maxWorkspaceRowsLand">12</integer>
    <integer name="config_minWorkspaceRowsLand">3</integer>
    <integer name="config_defaultWorkspaceColumnsLand">4</integer>
    <integer name="config_maxWorkspaceColumnsLand">12</integer>
    <integer name="config_minWorkspaceColumnsLand">3</integer>

    <!-- App drawer grid size -->
    <integer name="config_defaultAppRows">6</integer>
    <integer name="config_maxAppRows">12</integer>
    <integer name="config_minAppRows">3</integer>
    <integer name="config_defaultAppColumns">5</integer>
    <integer name="config_maxAppColumns">12</integer>
    <integer name="config_minAppColumns">3</integer>
    <integer name="config_defaultAppRowsLand">4</integer>
    <integer name="config_maxAppRowsLand">12</integer>
    <integer name="config_minAppRowsLand">3</integer>
    <integer name="config_defaultAppColumnsLand">7</integer>
    <integer name="config_maxAppColumnsLand">12</integer>
    <integer name="config_minAppColumnsLand">3</integer>

    <!-- App drawer widget list grid size -->
    <integer name="config_defaultWidgetRows">3</integer>
    <integer name="config_maxWidgetRows">6</integer>
    <integer name="config_minWidgetRows">1</integer>
    <integer name="config_defaultWidgetColumns">2</integer>
    <integer name="config_maxWidgetColumns">4</integer>
    <integer name="config_minWidgetColumns">1</integer>
    <integer name="config_defaultWidgetRowsLand">2</integer>
    <integer name="config_maxWidgetRowsLand">4</integer>
    <integer name="config_minWidgetRowsLand">1</integer>
    <integer name="config_defaultWidgetColumnsLand">3</integer>
    <integer name="config_maxWidgetColumnsLand">6</integer>
    <integer name="config_minWidgetColumnsLand">1</integer>

    <!-- More lennox ints -->
    <integer name="config_dockDefaultPages">1</integer>
    <integer name="config_dockMaxPages">5</integer>
    <integer name="config_dockMinPages">1</integer>

    <!-- Cell layout types -->
    <integer name="cell_layout_workspace">0</integer>
    <integer name="cell_layout_hotseat">1</integer>
    <integer name="cell_layout_folder">2</integer>
    <integer name="cell_layout_preview">3</integer>

    <!-- Actual cell count -->
    <integer name="target_cell_count_x">@integer/default_cell_count_x</integer>
    <integer name="target_cell_count_y">@integer/default_cell_count_y</integer>
    <!-- Default cell count that scales properly (will be used as a reference for scaling when different counts are used) -->
    <integer name="default_cell_count_x">4</integer>
    <integer name="default_cell_count_y">4</integer>

    <!-- Whether or not to use the wallpaper hack by default -->
    <bool name="config_workspaceDefaultWallpaperHack">false</bool>

<!-- Hotseat -->
    <bool name="hotseat_transpose_layout_with_orientation">true</bool>
    <integer name="hotseat_cell_count">5</integer>
    <integer name="hotseat_max_cell_count">7</integer>
    <integer name="hotseat_min_cell_count">3</integer>
    <integer name="hotseat_all_apps_index">2</integer>
    <!-- must be between 0 and 100 -->
    <integer name="hotseat_item_scale_percentage">100</integer>
    <integer name="hotseat_item_scale_percentage_max">150</integer>
    <integer name="hotseat_item_scale_percentage_min">50</integer>

    <!-- Wallpaper size -->
    <integer name="config_appDrawerTransparencyDefault">50</integer>

    <!-- must be between 0 and 100 -->
    <integer name="app_icon_scale_percentage_default">100</integer>
    <integer name="app_icon_scale_percentage_max">150</integer>
    <integer name="app_icon_scale_percentage_min">50</integer>

    <!-- Default theme colour -->
    <integer name="color_holo_blue">0xff33b5e5</integer>

</resources>
